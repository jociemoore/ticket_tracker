<table id="tickets-data">
  <thead>
    <tr>
      <th>Name</th>
      <th>Assignee</th>
      <th>Status</th>
      <th>Tags</th>
      <th> </th>
    </tr>
  </thead>
  <tbody>
    <% tickets.each do |ticket| %>
      <tr><%= render 'tickets/ticket', ticket: ticket %></tr>
    <% end %>
  </tbody>
</table>
